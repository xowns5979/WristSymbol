geom_errorbar(aes(ymin=accuracy-stderr, ymax=accuracy+stderr),
color="black",
width=.2,
size=0.5,
position=position_dodge(.9))
p+labs(x="",y="Accuracy(%)", fill="Arm Posture") +
theme(axis.text=element_text(size=11,family="sans"))
